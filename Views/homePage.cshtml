@using Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Cms.Core.Models.Blocks;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<HomePage>
@{
    Layout = "masterLayout.cshtml";
    ViewBag.Title = Model.Name;

    var currentUrl = $"{Context.Request.Scheme}://{Context.Request.Host}{Context.Request.Path}";
    var siteUrl = $"{Context.Request.Scheme}://{Context.Request.Host}";
}

@await Html.PartialAsync("HeroSlider")
@await Html.PartialAsync("ProjectCards")
@await Html.PartialAsync("WhyUs")
@await Html.PartialAsync("CustomerTestimonials")
@await Html.PartialAsync("BlogSection")
@await Html.PartialAsync("FAQ")



